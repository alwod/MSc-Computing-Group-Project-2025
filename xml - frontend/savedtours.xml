  android:layout_marginLeft="-3dp"
  android:layout_alignParentTop="true"
  android:layout_marginTop="-36dp"
  android:background="@drawable/edinburgh_t"
 />
  
<!-- drawable/edinburgh_t.xml -->
<vector
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:aapt="http://schemas.android.com/aapt"
  android:width="209dp"
  android:height="209dp"
  android:viewportWidth="209"
  android:viewportHeight="209"
 >
<group>
<clip-path
  android:pathData="M0 0H209V209H0V0Z"
/>
</group>
</vector>

<!-- Auto layout, grids, variables, and unit scale are not yet supported -->
<View
