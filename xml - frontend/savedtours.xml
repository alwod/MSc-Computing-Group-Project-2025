 />
<!--
  Font family: Inknut Antiqua
  Line height: 72sp
  -->
  
<!-- styles.xml -->
<style name="your_saved_">
<item name="android:textSize">28sp</item>
<item name="android:textColor">#0065BD</item>
</style>
  
<!-- strings.xml -->
<string name="your_saved_">
  Your Saved Tours
</string>

